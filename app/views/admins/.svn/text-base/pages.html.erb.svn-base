<table width="100%" border="0" cellpadding="3" cellspacing="1">
  <tr class="heading-color">
    <td align="left" valign="top" nowrap="nowrap"> Page Name </td>
    <td align="center" valign="top">Actions</td>
  </tr>
    <%
       if !@page.blank?
       @rowClass = 'white-bg'
       @page.each do |page|
       if @rowClass == 'white-bg'
        @rowClass = 'gray-bg1'
       else
         @rowClass = 'white-bg'
       end
    %>
  <tr class="<%= @rowClass %>">
    <td align="left"  valign="middle" nowrap="nowrap" class="heading6"><%= page.page_heading %></td>
    <td align="center" valign="middle" nowrap="nowrap"><a href="/admins/pages_edit/<%= page.id %>">Edit</a></td>
  </tr>
      <% end %>
      <% else %>
  <tr>
    <td align="center" colspan="2" valign="top" class="heading6">No Pages Present</td>
  </tr>
      <% end %>
</table>
