/* inner page */
.innermain {width:100%; float:left; background:#fff;}
.innerwrap {width:800px; margin:0 auto;  }
.contentmain { margin:5% 0 4% 0; background:#fff; padding:20px; border:1px solid #ccc6c6; height:350px; overflow-x:hidden;  overflow-y:scroll; }
.contentmain  h1 {width:800px; float:left; margin:0 0 8px 0; color:#e01f26; font-size:21px; font-weight:bold;}
.contentmain  h2 {width:800px; float:left; margin:0 0 8px 0; color:#181818; font-size:15px; font-weight:bold;}
.contentmain  p {width:800px; float:left; margin:0 0 8px 0; color:#4c4949; font-size:12px; font-weight:normal;}

.contentmainnew { margin:5% 0 4% 0; background:#fff; }
.contentmainnew  h1 {width:800px; float:left; margin:0 0 8px 0; color:#e01f26; font-size:21px; font-weight:bold;}
.contentmainnew  h2 {width:800px; float:left; margin:0 0 8px 0; color:#181818; font-size:15px; font-weight:bold;}
.contentmainnew  p {width:800px; float:left; margin:0 0 8px 0; color:#4c4949; font-size:12px; font-weight:normal;}



.footlink {width:100%; text-align:center; color:#4d4a4a; font-size:10px; margin:1% 0 0 0; float:left; }
.footlink a {font-size:11px; text-decoration:none; color:#4d4a4a;}
.footlink a:hover {font-size:11px; text-decoration:none; color:#4d4a4a;}
.contactleft {width:500px; float:left; margin:0 20px 0 0 0;}
.formrow {width:480px; float:left; padding:0 0 5px 0; margin:0 0 5px 0; font-family:Arial, Helvetica, sans-serif; font-size:12px;}
.formrow label {width:120px; float:left; margin:5px 0 0 0; color:#4c4949;}
.formrow input[type=text] {padding:7px; width:300px; color:#000; border:1px solid #4c4949; font-size:12px;}
.formrow input:focus[type=text] {padding:7px; width:300px; color:#000; border:1px solid #666; font-size:12px;}
.formrow select {padding:7px; width:315px; color:#000; border:1px solid #4c4949; font-size:12px;}
.formrow select:focus {padding:7px; width:315px; color:#000; border:1px solid #666; font-size:12px;}
.formrow input[type=submit] {background:#E01F26; height:30px; width:315px; line-height:30px; color:#fff; text-decoration:none; cursor:pointer; font-weight:bold; font-size:15px; border:none; font-family:Century Gothic, Arial, Helvetica, sans-serif;}
.formrow input:hover[type=submit] {background:#E01F26; height:30px; width:315px; line-height:30px; color:#fff; text-decoration:none; cursor:pointer; font-weight:bold; font-size:15px; border:none; font-family:Century Gothic, Arial, Helvetica, sans-serif;}


