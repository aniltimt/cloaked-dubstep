/* accordion */
#accordion{margin:0; padding:0; list-style:none; width:268px; float:left;}
#accordion li{width:268px; margin:0 0 1px 0;}
#accordion li a{display:block; width:184px; height:39px; line-height:39px; padding:0 60px; font-size:17px; font-weight:bold; color:#ffffff; text-decoration:none;}
#accordion li a.popular{background:url(../images/accorlink.png) no-repeat 0 0}
#accordion li a.popular:hover{background:url(../images/accorlink.png) no-repeat 0 0;}
#accordion li a.popularOver{background:url(../images/accorhover.png) no-repeat 0 0 !important}
#accordion li a.category{background:url(../images/accorlink.png) no-repeat 0 0;}
#accordion li a.category:hover{background:url(../images/accorlink.png) no-repeat 0 0;}
#accordion li a.categoryOver{background:url(../images/accorhover.png) no-repeat 0 0 !important}
#accordion li a.comment{background:url(../images/accorlink.png) no-repeat 0 0;}
#accordion li a.comment:hover{background:url(../images/accorlink.png) no-repeat 0 0 !important}
#accordion li a.commentOver{background:url(../images/accorhover.png) no-repeat 0 0;}
#accordion ul{background:#3e3e3e; width:224px; margin:0 0 0 58px; padding:0; display:none; border-top:1px solid #fff;z-index: 10;
position: fixed;}
#accordion ul li{height:22px; line-height:22px; width:224px; list-style:none}
#accordion ul li a{width:204px; height:22px; line-height:22px; padding:0 10px; border-bottom:1px solid #181818; text-indent:0; color:#fff; text-decoration:none; font-family:Arial,Helvetica,sans-serif; font-size:11px}
#accordion ul li a:hover{background:#e01f26; display:block}
#accordion ul li a.last{border-bottom:none}
.cat_selected {background:#e01f26;}
 .formrow input[type="password"] {border: 1px solid #4C4949;color: #000000;font-size: 12px;padding: 7px;width: 300px;}
.formrow input:hover[type=submit] {background:#E01F26; height:30px; width:315px; line-height:30px; color:#fff; text-decoration:none; cursor:pointer; font-weight:bold; font-size:15px; border:none; font-family:Century Gothic, Arial, Helvetica, sans-serif;}

